{
  "expo": {
    "name": "RNApp",
    "slug": "RNApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "privacy": "unlisted",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindslab.RNApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "My ReactNative demo",
    "android": {
      "package": "com.calitb.RNApp"
    }
  }
}
